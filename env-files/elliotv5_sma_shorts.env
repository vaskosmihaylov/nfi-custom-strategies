# ElliotV5_SMA_Shorts Strategy Environment
# Multi-Strategy Freqtrade Setup - ElliotV5_SMA_Shorts (Shorts-only parallel test)
# Running in parallel with ElliotV5_SMA (longs) for 30-day performance comparison

FREQTRADE__BOT_NAME=Vasko_ElliotV5_SMA_Shorts
FREQTRADE__STRATEGY=ElliotV5_SMA_Shorts

# Trading Configuration
FREQTRADE__TRADING_MODE=futures
FREQTRADE__MAX_OPEN_TRADES=8
# Note: Strategy enforces max 8 shorts via confirm_trade_entry callback

# Exchange Settings (Bybit)
FREQTRADE__EXCHANGE__NAME=bybit
FREQTRADE__EXCHANGE__KEY=Put_Your_Bybit_API_Key_Here
FREQTRADE__EXCHANGE__SECRET=Put_Your_Bybit_API_Secret_Here

# API Server Configuration
FREQTRADE__API_SERVER__ENABLED=true
FREQTRADE__API_SERVER__LISTEN_PORT=8080
FREQTRADE__API_SERVER__USERNAME=elliotv5_sma_shorts_user
FREQTRADE__API_SERVER__PASSWORD=elliotv5_sma_shorts_secure_password
FREQTRADE__API_SERVER__JWT_SECRET_KEY=ElliotV5_SMAShort sJWTSecretKey2024
FREQTRADE__API_SERVER__WS_TOKEN=ElliotV5_SMAShortsWSToken2024
FREQTRADE__API_SERVER__CORS_ORIGINS=https://freq.gaiaderma.com,http://freq.gaiaderma.com
FREQTRADE__API_SERVER__FORWARDED_ALLOW_IPS=*

# Telegram Settings (Optional)
FREQTRADE__TELEGRAM__ENABLED=false
FREQTRADE__TELEGRAM__TOKEN=123456789:YourTelegramBotToken
FREQTRADE__TELEGRAM__CHAT_ID=123456789

# Environment Settings
TZ=Europe/Istanbul
FREQTRADE__DRY_RUN=true
