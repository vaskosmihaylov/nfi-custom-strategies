# NoTankAI Strategy Environment
# Multi-Strategy Freqtrade Setup - NoTankAI

FREQTRADE__BOT_NAME=Vasko_NoTankAI
FREQTRADE__STRATEGY=NoTankAI_15

# Trading Configuration
FREQTRADE__TRADING_MODE=futures
FREQTRADE__MAX_OPEN_TRADES=6
FREQTRADE__FUTURES_MAX_OPEN_TRADES_LONG=4
FREQTRADE__FUTURES_MAX_OPEN_TRADES_SHORT=2

# Exchange Settings (Bybit)
FREQTRADE__EXCHANGE__NAME=bybit
FREQTRADE__EXCHANGE__KEY=Put_Your_Bybit_API_Key_Here
FREQTRADE__EXCHANGE__SECRET=Put_Your_Bybit_API_Secret_Here

# API Server Configuration
FREQTRADE__API_SERVER__ENABLED=true
FREQTRADE__API_SERVER__LISTEN_PORT=8080
FREQTRADE__API_SERVER__USERNAME=notankai_user
FREQTRADE__API_SERVER__PASSWORD=notankai_secure_password
FREQTRADE__API_SERVER__JWT_SECRET_KEY=NoTankAIJWTSecretKey2024
FREQTRADE__API_SERVER__WS_TOKEN=NoTankAIWSToken2024
FREQTRADE__API_SERVER__ROOT_PATH=/api/v1/notankai
FREQTRADE__API_SERVER__CORS_ALLOW_ORIGINS=["https://freq.gaiaderma.com","http://freq.gaiaderma.com"]
FREQTRADE__API_SERVER__FORWARDED_ALLOW_IPS=*

# Telegram Settings (Optional)
FREQTRADE__TELEGRAM__ENABLED=false
FREQTRADE__TELEGRAM__TOKEN=123456789:YourTelegramBotToken
FREQTRADE__TELEGRAM__CHAT_ID=123456789

# Environment Settings
TZ=Europe/Istanbul
FREQTRADE__DRY_RUN=true