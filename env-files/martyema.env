# MartyEMA Strategy Environment
# Multi-Strategy Freqtrade Setup - MartyEMA

FREQTRADE__BOT_NAME=Vasko_MartyEMA
FREQTRADE__STRATEGY=MartyEMA

# Trading Configuration
FREQTRADE__TRADING_MODE=futures
FREQTRADE__MAX_OPEN_TRADES=8
FREQTRADE__FUTURES_MAX_OPEN_TRADES_LONG=4
FREQTRADE__FUTURES_MAX_OPEN_TRADES_SHORT=4

# Exchange Settings (Bybit)
FREQTRADE__EXCHANGE__NAME=bybit
FREQTRADE__EXCHANGE__KEY=Put_Your_Bybit_API_Key_Here
FREQTRADE__EXCHANGE__SECRET=Put_Your_Bybit_API_Secret_Here

# API Server Configuration
FREQTRADE__API_SERVER__ENABLED=true
